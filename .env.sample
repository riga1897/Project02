
# Переменные окружения для приложения поиска вакансий
# Скопируйте этот файл в .env и укажите свои значения

# SuperJob API Token
# Получите токен на https://api.superjob.ru/register/
# Инструкция: https://api.superjob.ru/info/
SUPERJOB_API_KEY=your_superjob_api_key_here

# Настройки кэширования (в секундах)
CACHE_TTL=3600

# Настройки логирования (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Optional: Set to disable progress bars in tests
DISABLE_TQDM=0
